//notacion de corchetes cuando el nombre de la propiedad tiene espacios
//notacion de puntos, cuando el nombre de la propiedad no tiene espacios
//cuando se trata de usar variables solo podemos usar notacion de corchetes

var Guitarra = {
  "Color": "blanco y negro",
  "a√±o": 1997,
  "cuerdas": "si",
}

function verificarPro(obj, propiedad){
  if (obj.hasOwnProperty(propiedad)){
    return "propiedad: " + obj[propiedad];
  } else {return "este objeto no tiene esta propiedad";}
}

console.log(verificarPro(Guitarra,"Color"));
console.log(verificarPro(Guitarra,"numero"));

var mochila = {
  color: "verde",
  marca: "random",
  contenido: ["libro", "termo", "laptop"],
};

console.log(mochila.contenido);
console.log(mochila.color)
mochila.color = "red"
console.log(mochila.color)
mochila.contenido.push("lapiz")
console.log(mochila.contenido);
delete mochila.marca;
console.log(mochila);
